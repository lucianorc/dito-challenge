<p id="notice"><%= notice %></p>

<h1>Scores</h1>

<table>
  <thead>
    <tr>
      <th>Uid</th>
      <th>Username</th>
      <th>Points</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scores.each do |score| %>
      <tr>
        <td><%= score.uid %></td>
        <td><%= score.username %></td>
        <td><%= score.points %></td>
        <td><%= score.date %></td>
        <td><%= link_to 'Show', score %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
